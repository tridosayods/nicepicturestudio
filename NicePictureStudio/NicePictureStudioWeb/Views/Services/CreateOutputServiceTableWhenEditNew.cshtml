<div id="tblOutput">
    @if (@ViewBag.ListOutputServices != null)
    {
        <div class="form-group">
            <table class="table table-striped">
                <tr><th>ชื่อ</th><th>รายละเอียด</th><th>จำนวนชุด</th><th>ราคา</th><th></th></tr>
                @foreach (NicePictureStudio.Models.OutputServiceViewModel item in ViewBag.ListOutputServices)
                {
                    <tr>
                        <td>@item.Name</td>
                        <td>@item.Description</td>
                        <td>@item.OutputQuantity</td>
                        <td>@item.Price</td>
                        <td>@Ajax.ActionLink("Remove", "RemoveOutputWhenEditNew", new { Id = @item.Id, serviceType = @ViewBag.OutputServiceType,IsEditTable = @ViewBag.IsEditTable }, new AjaxOptions { HttpMethod = "POST", InsertionMode = InsertionMode.Replace, UpdateTargetId = "tblOutput" })</td>
                    </tr>
                }
            </table>
        </div>
    }
</div>
