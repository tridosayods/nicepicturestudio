@model IEnumerable<NicePictureStudio.Models.ApplicationUser>

@{
    ViewBag.Title = "การกำหนดสิทธิผู้ใช้ระบบ";
}

<h2>การกำหนดสิทธิผู้ใช้ระบบ</h2>

<p>
    @Html.ActionLink("สร้างใหม่", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @*@Html.DisplayNameFor(model => model.UserName)*@
            @Html.Label("ชื่อผู้ใช้ระบบ")
        </th>
        <th>

        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.ActionLink("แก้ไข", "Edit", new { id = item.Id }) |
                @Html.ActionLink("ดูรายละเอียด", "Details", new { id = item.Id }) |
