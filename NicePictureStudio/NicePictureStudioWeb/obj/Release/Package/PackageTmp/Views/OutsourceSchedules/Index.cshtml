@model IEnumerable<NicePictureStudio.App_Data.OutsourceSchedule>


    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    @{Html.RenderAction("OutsourceScheduler", "OutsourceSchedules");}
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.StartTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EndTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OutsourceId)
