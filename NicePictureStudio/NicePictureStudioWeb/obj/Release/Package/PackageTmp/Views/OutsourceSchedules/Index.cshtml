                @Html.DisplayFor(modelItem => item.EndTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OutsourceId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OutsourceServiceId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
